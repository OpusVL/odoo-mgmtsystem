<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Feedback Types: Nonconformity, Good Practice, Improvement Opportunity, Observation, ... -->
        <record id="feedback_type_nc" model="mgmtsystem.feedback.type">
            <field name="name">Nonconformity</field>
            <field name="active" eval="True"/>
        </record>
        <record id="feedback_type_gp" model="mgmtsystem.feedback.type">
            <field name="name">Good Practice</field>
            <field name="active" eval="True"/>
        </record>
        <record id="feedback_type_io" model="mgmtsystem.feedback.type">
            <field name="name">Improvement Opportunity</field>
            <field name="active" eval="True"/>
        </record>
        <record id="feedback_type_ob" model="mgmtsystem.feedback.type">
            <field name="name">Observation</field>
            <field name="active" eval="True"/>
        </record>

        <!-- Feedback Severity: Critical, Major, Minor, Invalid, ... -->
        <record id="feedback_severity_cri" model="mgmtsystem.feedback.severity">
            <field name="name">Critical</field>
            <field name="active" eval="True"/>
        </record>
        <record id="feedback_severity_maj" model="mgmtsystem.feedback.severity">
            <field name="name">Major</field>
            <field name="active" eval="True"/>
        </record>
        <record id="feedback_severity_min" model="mgmtsystem.feedback.severity">
            <field name="name">Minor</field>
            <field name="active" eval="True"/>
        </record>
        <record id="feedback_severity_no" model="mgmtsystem.feedback.severity">
            <field name="name">Unfounded</field>
            <field name="active" eval="True"/>
        </record>
        <record id="feedback_severity_na" model="mgmtsystem.feedback.severity">
            <field name="name">Not applicable</field>
            <field name="active" eval="True"/>
        </record>

        <!-- Origins (default data, not demo!) -->
        <record id="nonconformity_origin_complaint" model="mgmtsystem.nonconformity.origin">
            <field name="name">Complaint</field>
        </record>
        <record id="nonconformity_origin_intaudit" model="mgmtsystem.nonconformity.origin">
            <field name="name">Internal Audit</field>
        </record>
        <record id="nonconformity_origin_extaudit" model="mgmtsystem.nonconformity.origin">
            <field name="name">External Audit</field>
        </record>
        <record id="nonconformity_origin_measure" model="mgmtsystem.nonconformity.origin">
            <field name="name">Analysis or measurement</field>
        </record>
        <record id="nonconformity_origin_review" model="mgmtsystem.nonconformity.origin">
            <field name="name">Management review</field>
        </record>
        <record id="nonconformity_origin_measure" model="mgmtsystem.nonconformity.origin">
            <field name="name">Other feedback</field>
        </record>
        
    </data>
</openerp>
